function onOpen() {
  
  SpreadsheetApp.getUi()
  .createMenu('⚙️ Kelola Administrasi')
  .addItem('Kirim Pesan (Pastikan Pada Sheet REG WA)', 'sendWhatsapp') // label for menu item, name of function to run.
  .addItem('Buka Tes TPA', 'OpenWeb') // label for menu item, name of function to run.
  .addToUi();  
  
}
